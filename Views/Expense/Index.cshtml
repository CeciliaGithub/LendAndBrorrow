@model IEnumerable<InAndOut.Models.Expense>
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary"> Expense</h2>
        </div>
        <div class="col-6">
            <a asp-area="" asp-controller="Expense" asp-action="Create" class="btn btn-primary"> Create Expense</a>
        </div>
    </div>
</div>
@if (Model.Count() > 0)
{

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th> Expense Name</th>
            <th> Price </th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var Expense in Model)
        {
            <tr>
                <td width="50%">@Expense.ExpenseName</td>
                <td width="50%">@Expense.Price</td>
              
            </tr>
        }
    </tbody>

</table>
}
else
{
<p>No items created yet</p>
}

<br />